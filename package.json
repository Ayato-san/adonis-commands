{
  "name": "@ayato-san/adonis-commands",
  "type": "module",
  "version": "1.2.2",
  "description": "My custom commands for AdoniJS",
  "author": "Ayato-san <ayatosan.pro@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Ayato-san/adonis-commands#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayato-san/adonis-commands.git"
  },
  "bugs": {
    "url": "https://github.com/Ayato-san/adonis-commands/issues"
  },
  "keywords": [
    "adonisjs",
    "adonis"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js"
  },
  "main": "build/index.js",
  "files": [
    "build/commands",
    "build/configure.d.ts",
    "build/configure.js",
    "build/index.d.ts",
    "build/index.js",
    "build/services",
    "build/src",
    "build/stubs"
  ],
  "engines": {
    "node": ">= 20.11.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "prebuild": "npm run format && npm run lint:fix",
    "build": "npm run clean && tsc",
    "postbuild": "npm run copy:templates && npm run index:commands",
    "prepublishOnly": "npm run build",
    "release": "npx release-it",
    "test": "cross-env NODE_DEBUG=adonisjs:core c8 npm run test:quick",
    "test:quick": "node --loader=ts-node/esm --enable-source-maps --experimental-import-meta-resolve bin/test.ts --force-exit",
    "index:commands": "adonis-kit index build/commands"
  },
  "dependencies": {
    "local-pkg": "^0.5.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.13.1",
    "@adonisjs/lucid": "^21.2.0",
    "@ayato-san/tooling-configs": "^2.0.2",
    "@japa/assert": "^3.0.0",
    "@japa/expect-type": "^2.0.2",
    "@japa/file-system": "^2.3.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.26",
    "@types/node": "^22.5.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "prettier": "@ayato-san/tooling-configs/prettier/default",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
