{
  "name": "@ayato-san/adonis-commands",
  "type": "module",
  "version": "0.0.1",
  "description": "My custom commands for AdoniJS",
  "author": "Ayato-san <ayatosan.pro@gmail.com>",
  "license": "MIT",
  "keywords": [
    "adonisjs",
    "adonis"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js"
  },
  "main": "build/index.js",
  "files": [
    "build/commands",
    "build/configure.d.ts",
    "build/configure.js",
    "build/helpers",
    "build/index.d.ts",
    "build/index.js",
    "build/src",
    "build/stubs"
  ],
  "engines": {
    "node": ">= 20.11.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prebuild": "npm run format && npm run lint:fix",
    "build": "npm run clean && tsc",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "postbuild": "npm run copy:templates",
    "prepublishOnly": "npm run build",
    "release": "npx release-it",
    "test": "cross-env NODE_DEBUG=adonisjs:core c8 npm run quick:test"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/core": "^6.10.1",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@ayato-san/tooling-configs": "^1.0.0",
    "@japa/assert": "^3.0.0",
    "@japa/expect-type": "^2.0.2",
    "@japa/file-system": "^2.3.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.5.28",
    "@types/node": "^20.14.2",
    "c8": "^9.1.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "release-it": "^17.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@ayato-san/tooling-configs/prettier",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
